#: venv/lib/python3.12/site-packages/django/contrib/postgres/apps.py:54
msgid "PostgreSQL extensions"
msgstr "Estensioni per PostgreSQL"

#: venv/lib/python3.12/site-packages/django/contrib/postgres/fields/array.py:21
#: venv/lib/python3.12/site-packages/django/contrib/postgres/forms/array.py:17
#: venv/lib/python3.12/site-packages/django/contrib/postgres/forms/array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "L'elemento %(nth)s dell'array non è stato convalidato:"

#: venv/lib/python3.12/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Gli array annidati devono avere la stessa lunghezza."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Mappa di stringhe a stringhe/null"

#: venv/lib/python3.12/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "Il valore di \"%(key)s\" non è una stringa o nullo."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "Caricamento dati JSON fallito."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "L'input deve essere un dizionario JSON."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:42
msgid "Enter two valid values."
msgstr "Inserisci due valori validi."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:44
msgid "The start of the range must not exceed the end of the range."
msgstr ""
"Il valore iniziale dell'intervallo non può essere superiore al valore finale."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:99
msgid "Enter two whole numbers."
msgstr "Inserisci due numeri interi."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:105
msgid "Enter two numbers."
msgstr "Inserisci due numeri."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:111
msgid "Enter two valid date/times."
msgstr "Inserisci due valori data/ora validi."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:117
msgid "Enter two valid dates."
msgstr "Inserisci due date valide."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"La lista contiene %(show_value)d oggetto, non dovrebbe contenerne più di "
"%(limit_value)d."
msgstr[1] ""
"La lista contiene %(show_value)d elementi, e dovrebbe contenerne al massimo "
"%(limit_value)d."
msgstr[2] ""
"La lista contiene %(show_value)d elementi, e dovrebbe contenerne al massimo "
"%(limit_value)d."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"La lista contiene %(show_value)d oggetto, non dovrebbe contenerne meno di "
"%(limit_value)d."
msgstr[1] ""
"La lista contiene %(show_value)d oggetti, e dovrebbe contenerne almeno "
"%(limit_value)d."
msgstr[2] ""
"La lista contiene %(show_value)d oggetti, e dovrebbe contenerne almeno "
"%(limit_value)d."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Alcune chiavi risultano mancanti: %(keys)s"

#: venv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Sono state fornite alcune chiavi sconosciute: %(keys)s"

#: venv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:81
#, python-format
msgid ""
"Ensure that the upper bound of the range is not greater than %(limit_value)s."
msgstr ""
"Assicurarsi che il limite superiore dell'intervallo non sia maggiore di "
"%(limit_value)s."

#: venv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:90
#, python-format
msgid ""
"Ensure that the lower bound of the range is not less than %(limit_value)s."
msgstr ""
"Assicurarsi che il limite inferiore dell'intervallo non sia inferiore a "
"%(limit_value)s."
