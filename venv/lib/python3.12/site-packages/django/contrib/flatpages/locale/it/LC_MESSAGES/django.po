#: venv/lib/python3.12/site-packages/django/contrib/flatpages/admin.py:13
msgid "Advanced options"
msgstr "Opzioni avanzate"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/apps.py:8
msgid "Flat Pages"
msgstr "Flat Pages"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/forms.py:11
#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Per esempio: '/about/contact'. Assicurati che inizi e finisca con uno slash."

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/forms.py:20
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Questo valore deve contenere solo lettere, numeri, punti, underscore, "
"trattini, barre diagonali o tilde."

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/forms.py:34
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr "Per esempio: '/about/contact'. Assicurati che inizi con uno slash."

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/forms.py:47
msgid "URL is missing a leading slash."
msgstr "Manca una barra iniziale nella URL."

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/forms.py:52
msgid "URL is missing a trailing slash."
msgstr "Manca una barra finale nella URL."

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/forms.py:69
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "La flatpage con url %(url)s esiste già per il sito %(site)s"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "titolo"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "contenuto"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "abilita commenti"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "nome template"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Esempio: \"flatpages/contact_page.html\". Se non specificato, il sistema "
"userà \"flatpages/default.html\"."

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "registrazione obbligatoria"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:25
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Se selezionato, solo gli utenti che hanno effettuato l'accesso potranno "
"vedere la pagina."

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:29
msgid "sites"
msgstr "siti"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:33
msgid "flat page"
msgstr "flat page"

#: venv/lib/python3.12/site-packages/django/contrib/flatpages/models.py:34
msgid "flat pages"
msgstr "flat pages"
